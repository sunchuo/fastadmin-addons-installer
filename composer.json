{
    "name": "fastadmin/addons-installer",
    "description": "Fastadmin插件安装器",
    "type": "composer-plugin",
    "autoload": {
        "psr-4": {
            "Fastadmin\\AddonsInstaller\\": "src/"
        }
    },
    "extra": {
        "class": "Fastadmin\\AddonsInstaller\\Plugin"
    },
    "require": {
        "composer-plugin-api": "^1.1",
        "fastadmin/addons": "dev-master"
    },
    "repositories": [
        {
            "type": "git",
            "url":  "git@github.com:sunchuo/fastadmin-addons.git"
        }
    ],
    "require-dev": {
        "composer/composer": "^1.9"
    }
}

